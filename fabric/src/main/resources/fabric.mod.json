{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/a455jldvmsrwll1a/AuthorisedKeysMC",
        "issues": "https://github.com/a455jldvmsrwll1a/AuthorisedKeysMC/issues",
        "homepage": "https://github.com/a455jldvmsrwll1a/AuthorisedKeysMC"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "server": [
            "ph.jldvmsrwll1a.authorisedkeysmc.AuthorisedKeysMC"
        ],
        "client": [
            "ph.jldvmsrwll1a.authorisedkeysmc.AuthorisedKeysMCClient"
        ],
        "modmenu": [
            "ph.jldvmsrwll1a.authorisedkeysmc.ModMenuIntegration"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.client.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "~${minecraft_version}",
        "java": ">=${java_version}"
    },
    "accessWidener": "${mod_id}.accesswidener",
    "suggests": {
        "modmenu": ">=${modmenu_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.website": "https://github.com/a455jldvmsrwll1a/AuthorisedKeysMC"
            },
            "update_checker": false
        }
    }
}
  